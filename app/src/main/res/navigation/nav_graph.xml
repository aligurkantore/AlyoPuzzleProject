<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/hashFragment">

    <fragment
        android:id="@+id/hashFragment"
        android:name="com.example.alyopuzzleproject.ui.fragment.hash.HashFragment"
        android:label="fragment_hash"
        tools:layout="@layout/fragment_hash" >
        <action
            android:id="@+id/action_hashFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.alyopuzzleproject.ui.fragment.result.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" />
</navigation>